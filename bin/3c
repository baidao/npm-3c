#!/usr/bin/env coffee

program = require 'commander'
argv = require('optimist').argv
program
  .version('0.0.1')
  .usage('[options]')
  .description('3c - doc mock check')
  .option('-i, --init', '初始化')
  .option('-d, --doc', '文档')
  .option('-m, --mock', 'mock服务器')
  .option('-c, --check', '服务器数据监测')
  .parse(process.argv)

require('../index').exec(argv)

